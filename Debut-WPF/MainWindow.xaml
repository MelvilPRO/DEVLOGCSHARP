<Window x:Class="Debut_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Debut_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="ButtonLireCSV" Content="Lire le fichier CSV" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Height="45" Width="780" Click="ButtonLireCSV_Click"/>
        <TextBlock x:Name="TBlockDisplayMoyenne" HorizontalAlignment="Center" Margin="0,60,0,0" TextWrapping="Wrap" Text="La moyenne s'affichera ici" VerticalAlignment="Top" Height="364" Width="780"/>

    </Grid>
</Window>
